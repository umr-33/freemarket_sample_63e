    <%= form_for(@item, local: true) do |form| %>

      
      <%= form.text_field :title, placeholder: "title" %>
      <%= form.text_field :description, placeholder: "description" %>
      <%= form.text_field :prefecture_id, placeholder: "prefecture_id" %>
      <%= form.text_field :shipping_charger_id, placeholder: "shipping_charger_id" %>
      <%= form.text_field :shipping_method_id, placeholder: "shipping_method_id" %>
      <%= form.text_field :days_to_ship_id, placeholder: "days_to_ship_id" %>
      <%= form.text_field :item_status_id, placeholder: "item_status_id" %>
      <%= form.text_field :trade_status_id, placeholder: "trade_status_id" %>
      <%= form.text_field :brand_id, placeholder: "brand" %>
      <%= form.text_field :category_id, placeholder: "category" %>
      <%= form.file_field :images, class: 'file-field',name: 'item_images[image][]', multiple: true %>
      <%= form.submit "SEND" %>


      
    <% end %>