<header class="user_new user_new3">
  <%=render partial: "users/new/uheader"%>
</header>
<div class="contents_wrapper3">
<section>
  <h3>お届け先住所入力</h3>
  <form action="">
  <div class="form__inputs">
    <p>お名前<span class="white_in_red">必須</span></p>
    <input type="text" placeholder="例）山田"/>
    <input type="text" placeholder="例）彩">
    <p>お名前カナ<span class="white_in_red">必須</span></p>
    <input type="text" placeholder="例）ヤマダ">
    <input type="text" placeholder="例）アヤ">
    <p>郵便番号<span class="white_in_red">必須</span></p>
    <input type="text" placeholder="例）123-456">
    <p>都道府県<span class="white_in_red">必須</span></p>
    <div class="select_wrap">
      <select name="prefecture" id="">
        <option value="">北海道</option>
        <option value="">青森県</option>
      </select>
      <i class="fas fa-chevron-down"></i>
    </div>
    <p>市区町村<span class="white_in_red">必須</span></p>
    <input type="text" placeholder="例）横浜市緑区">
    <p>番地<span class="white_in_red">必須</span></p>
    <input type="text" placeholder="例）青山1-1-1">
    <p>建物名<span class="white_in_gray">任意</span></p>
    <input type="text" placeholder="例）柳ビル103">
    <p>電話<span class="white_in_gray">任意</span></p>
    <input type="text" placeholder="例）09012345678">
    <%=link_to new4_users_path do%>
    <button type="button" class="white_in_red">次へ進む</button>
    <%end%>
  </div>
  </form>
</section>
</div>
<%= render partial: "users/new/ufooter" %>